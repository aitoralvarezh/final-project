   const me = useUser();\n\n    if (!me) {\n        return (\n            <div className=\"banner\">\n                <div>\n                   hello \n                </div>\n                <div>\n                    <Register />\n                    <Account />\n                </div>\n            </div>\n        )\n    }\n\n    return (\n        <div className=\"banner\">\n            <LastArticles />\n        </div>\n    )\n}\n\n\nexport default Banner;","/home/hab01/Documents/develop/final-project/client/src/welcome/landing.js",["124","125","126"],"import { useUser } from '../usercontext';\nimport ReadArticle from '../routes/articles/readarticle';\nimport LastArticles from '../routes/articles/lastarticle';\nimport NewArticle from '../routes/articles/createbutton';\nimport Banner from './banner'\nimport './main.css'\n\n\nimport { Route } from 'react-router-dom';\nimport SelectedArticle from '../routes/articles/selectedarticle'\nimport CreateArticles from '../routes/articles/createarticle';\n\nfunction MainPage({ articles }) {\n    const me = useUser();\n    if (!me) {\n        return (\n\n            <div className=\"selected-articles\">\n                <Banner />\n                <LastArticles />\n            </div>\n\n        )\n    }\n\n    return (\n        <div>\n            <div >\n                <Banner>\n                    <LastArticles />\n                </Banner>\n            </div>\n            <div className=\"main-page\">\n                <div className=\"new-articles\">\n                    <ReadArticle />\n                    <NewArticle />\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default MainPage;","/home/hab01/Documents/develop/final-project/client/src/welcome/Carousel.js",[],"/home/hab01/Documents/develop/final-project/client/src/routes/articles/createarticle.js",["127","128"],"import { useState, useRef } from 'react';\nimport { useUser, useSetUser } from '../../usercontext';\nimport { createArticles, useTopics } from '../../api'\nimport { useHistory } from 'react-router-dom';\n\nfunction CreateArticles() {\n    const history = useHistory();\n\n    const { user: me, token } = useUser()\n    const setMe = useSetUser()\n    const theInput = useRef();\n    const topics = useTopics()\n\n    const [preview, setPreview] = useState();\n    const [title, setTitle] = useState('');\n    const [content, setContent] = useState('');\n    const [topicId, setTopicId] = useState('');\n    const [visible, setVisible] = useState('1');\n\n\n    const handleSubmit = async e => {\n        e.preventDefault();\n        const image = e.target.image.files[0];\n\n        const data = await createArticles(token, topicId, image, title, content, visible);\n\n        history.push('/articles/read/' + data[0].id);\n\n\n        if (data) {\n            return data\n        }\n    }\n    const handleClick = e => {\n        theInput.current.click()\n    }\n    const handlePick = e => {\n        const reader = new FileReader()\n        reader.onloadend = () => setPreview(reader.result)\n        reader.readAsDataURL(e.target.files[0])\n    }\n    const style = preview && { backgroundImage: 'url(' + preview + ')' }\n\n\n    if (!topics) return 'loading...'\n\n    return (\n        <div>\n            <form className=\"profile-form\" onSubmit={handleSubmit}>\n                <div\n                    className=\"avatar showAvatar\"\n                    style={style}\n                    onClick={handleClick}\n                    value={preview}\n\n                >\n                </div>\n                <input\n                    className=\"hide\"\n                    name=\"image\"\n                    type='file'\n                    accept=\"image/*\"\n                    ref={theInput}\n                    onChange={handlePick}\n                />\n                <div className=\"data-input\">\n                    <label >\n                        <div className=\"name\">Título</div>\n                        <input\n                            name=\"title\"\n                            value={title}\n                            onChange={e => setTitle(e.target.value)}\n                        />\n                    </label>\n                    <label >\n                        <div className=\"name\">topic</div>\n                        <select\n                            name=\"topicId\"\n                            value={topicId}\n                            onChange={e => setTopicId(e.target.value)}\n                        >\n                            {topics.map(topic =>\n                                <option\n                                    key={topic}\n                                    value={topic.id}\n                                >\n                                    {topic.name}\n                                </option>\n                            )}\n                        </select>\n                    </label>\n                    <label >\n                        <div className=\"name\">visible</div>\n                        <select\n                            name=\"visible\"\n                            value={visible}\n                            onChange={e => setVisible(e.target.value)}>\n                            <option value=\"1\" selected>Público</option>\n                            <option value=\"0\">Privado</option>\n\n                        </select>\n                    </label>\n                    <label>\n                        <div className=\"name\">Descripción</div>\n                        <textarea\n                            name=\"content\"\n                            value={content}\n                            onChange={e => setContent(e.target.value)}\n                        />\n                    </label>\n                </div>\n                <button className=\"\">Guardar</button>\n            </form>\n        </div >\n    )\n}\n\nexport default CreateArticles;\n","/home/hab01